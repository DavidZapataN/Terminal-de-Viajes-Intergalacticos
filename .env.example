# API Configuration (will be ALB endpoint when backend is deployed)
VITE_API_BASE_URL=http://your-alb-endpoint.us-east-1.elb.amazonaws.com
VITE_API_VERSION=v1

# AWS Configuration
VITE_AWS_REGION=us-east-1

# Authentication (JWT handled by NestJS backend)
# No frontend JWT secret needed - tokens managed by backend

# Feature Flags
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_ANALYTICS=true

# Third-party Services
VITE_SENTRY_DSN=your-sentry-dsn

